  "type": "commonjs",
  "bugs": {
    "url": "https://github.com/Emilyempa/Fullstack-Wordle/issues"
  },
  "homepage": "https://github.com/Emilyempa/Fullstack-Wordle#readme",
  "devDependencies": {
    "cypress": "^14.3.0"
  },
  "engines": {
    "node": "20.x"
  }
}
